@{
    ViewData["Title"] = "Home Page";
}
<h2 class="Error">@ViewBag.Error</h2>
<br>
<a href='@Url.Action("SelectIndumentaria")'><input type="button" value="Seleccionar indumentaria"></input></a>
<br>
<br>
<h2>
    @foreach (string Equipo in Equipos.EquiposIndumentaria.Keys)
{
    
    <table>
    <tr>
    <td>@Equipo
    <img class="Ropa" src="/Imagenes/@Equipos.EquiposIndumentaria[Equipo].Camiseta">
    <img class="Ropa" src="/Imagenes/@Equipos.EquiposIndumentaria[Equipo].Pantalon">
    <img class="Ropa" src="/Imagenes/@Equipos.EquiposIndumentaria[Equipo].Medias">
    </td>
    </tr>
    </table>
    <br>
    
}
</h2>

